{
  "name": "@langgraph-rag/db",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "node -r @neondatabase/serverless/config -e 'require(\"./dist/migrate.js\").migrate()'"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.5",
    "drizzle-orm": "^0.33.0",
    "pgvector": "^0.1.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "drizzle-kit": "^0.20.0",
    "typescript": "^5.3.3"
  }
}


